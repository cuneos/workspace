  "description": "Enabler for build high-quality and scalable software",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nx affected --target=build --parallel",
    "build:all": "nx run-many --target=build --all --parallel",
    "test": "nx affected --target=test --parallel",
    "test:all": "nx run-many --target=test --all --parallel",
    "lint": "nx affected --target=lint --parallel",
    "lint:all": "nx run-many --target=lint --all --parallel",
    "lint:staged": "lint-staged",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "graph": "nx graph",
    "prepare": "husky install",
    "release": "nx affected --target=release"
  },
  "author": {
    "name": "Stefan Karlsson",
    "email": "iam@stefan-karlsson.se"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cuneos/workspace.git"
  },
  "bugs": {
    "url": "https://github.com/cuneos/workspace/issues"
  },
  "homepage": "https://github.com/cuneos/workspace#readme",
  "keywords": [
    "domain driven design",
    "scalable software",
    "high-quality software",
    "software architecture",
    "software design"
  ],
  "dependencies": {
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
